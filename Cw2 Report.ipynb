    "char_input = Input(shape=(maxlen,), dtype='int32', name='char_input')\n",
    "char_embedding = layers.Embedding(char_num_words+1, output_dim)(char_input)\n",
    "encoded_char_embedding = layers.LSTM(32)(char_embedding)\n",
    "\n",
    "\n",
    "word_input = Input(shape=(maxlen,), dtype='int32', name='word_input')\n",
    "word_embedding = layers.Embedding(num_words+1, output_dim)(word_input)\n",
    "encoded_word_embedding = layers.LSTM(32)(word_embedding)\n",
    "\n",
    "\n",
    "concatenated = layers.concatenate([encoded_char_embedding, encoded_word_embedding], axis=-1)\n",
    "answer = layers.Dense(3, activation='softmax')(concatenated)\n",
    "\n",
    "model = Model([char_input, word_input], answer)\n",
    "model.compile(optimizer='rmsprop', loss='categorical_crossentropy', metrics=['accuracy'])\n",
